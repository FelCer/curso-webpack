{
  "name": "curso-wepack",
  "version": "1.0.0",
  "description": "Curso de webpack, aprendiendo todo.",
  "main": "index.js",
  "scripts": {
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:dev": "webpack serve --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:img": "webpack --config ./url-loader-img/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js"
  },
  "author": "Luis Felipe Cerero Garcia",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "5.0.1",
    "file-loader": "^6.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "1.3.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.5.1",
    "webpack-cli": "4.2.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
